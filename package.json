{
  "name": "@jawg/types",
  "version": "1.0.5",
  "description": "Shared TypeScript definitions for Jawg Maps projects",
  "types": "./index.d.ts",
  "type": "module",
  "typeScriptVersion": "2.3",
  "scripts": {
    "docs": "typedoc --out docs index.d.ts",
    "start": "rollup -c -w",
    "test": "tsc --esModuleInterop --noEmit test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jawg/types.ts.git"
  },
  "keywords": [
    "api",
    "typescript",
    "maps"
  ],
  "author": "Jawg Maps <contact@jawg.io>",
  "license": "MIT",
  "files": [
