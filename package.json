    "README.md",
    "tsconfig.json",
    "index.d.ts",
    "packages.json"
  ],
  "bugs": {
    "url": "https://github.com/jawg/types.ts/issues"
  },
  "homepage": "https://jawg.github.io/types.ts",
  "devDependencies": {
    "prettier": "^3.1.1",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-serve": "^1.1.0",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.7.3",
    "@types/mapbox-gl": "^1.13.2",
    "maplibre-gl": "^3.6.2"
  }
