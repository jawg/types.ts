{
  "name": "@jawg/types",
  "version": "1.0.0",
  "description": "Shared TypeScript definitions for Jawg Maps projects",
  "types": "./index.d.ts",
  "typeScriptVersion": "2.3",
  "scripts": {
    "docs": "typedoc --out docs index.d.ts && touch docs/.nojekyll",
    "lint": "dtslint",
    "start": "rollup -c -w",
    "test": "tsc --noEmit test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jawg/types.ts.git"
  },
  "keywords": [
    "api",
    "typescript",
    "maps"
  ],
  "author": "Jawg Maps <contact@jawg.io>",
  "license": "MIT",
  "files": [
    "src/*",
    "README.md",
    "tsconfig.json",
    "index.d.ts",
    "packages.json"
  ],
  "bugs": {
    "url": "https://github.com/jawg/types.ts/issues"
  },
  "homepage": "https://github.com/jawg/types.ts#readme",
  "devDependencies": {
    "dtslint": "^4.1.1",
    "prettier": "^2.3.2",
    "rollup": "^2.52.8",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.7.3",
    "@types/mapbox-gl": "^2.3.1",
    "@types/maplibre-gl": "^1.13.1"
  }
}
